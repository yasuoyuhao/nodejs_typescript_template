    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.2.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/multer-s3": "^2.7.6",
    "@types/nodemailer": "^4.6.2",
    "@types/request": "^2.47.1",
    "@types/winston": "^2.3.9",
    "amazon-s3-uri": "0.0.3",
