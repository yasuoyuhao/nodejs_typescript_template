{
  "name": "yasuoyuhao_nodejs_typescript_template",
  "version": "1.0.0",
  "description": "server for nodejs and use typescript",
  "main": "server.js",
  "scripts": {
    "build": "tsc && apidoc -i lib/routes -o ./dist/public",
    "dev": "apidoc -i ./lib/routes -o ./lib/public && ts-node ./lib/server.ts",
    "start": ". ./startup.sh",
    "prod": "npm run build && rm -rf lib && npm run start"
  },
